<%= form_with(model: restaurant) do |form| %>
  
  <div class="container">
  <% if restaurant.errors.any? %>
    <div id="error_explanation">
      <!--<h2><%= pluralize(restaurant.errors.count, "error") %> prohibited this restaurant from being saved:</h2>-->
      <ul>
        <% restaurant.errors.each do |error| %>
        <div class="alert alert-danger" role="alert">
          <li><%= error.full_message %></li>
        </div>
        <% end %>
      </ul>
    </div>
  <% end %>
</div>


<div class="container" id="fclass">
<div class="form-class">
<form>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="form-group">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="form-group">
    <%= form.label :phone_no %>
    <%= form.number_field :phone_no %>
  </div>
  <div class="form-group">
    <%= form.label :address %>
    <%= form.text_area :address %>
  </div>
  <div class="form-group" style="margin:10px 0">
    <%= form.label :start_time %>
    <%= form.time_select :start_time %>
  </div>
  <div class="form-group" style="margin:10px 0">
    <%= form.label :end_time %>
    <%= form.time_select :end_time %>
  </div>
  <%= form.submit class:"btn btn-primary" %>
</form>
<% end %>
</div>
</div>
